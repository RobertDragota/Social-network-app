<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@LoginStyle.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="com.socialnetwork.map_toysocialnetwork.Controllers.LoginController">
   <children>
      <TextField fx:id="UserNameTextFiled" layoutX="302.0" layoutY="52.0" prefHeight="34.0" prefWidth="216.0" promptText="ex:Robert1234" styleClass="rounded-textfield" stylesheets="@LoginStyle.css" />
      <Button fx:id="LoginButton" layoutX="215.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="49.0" prefWidth="165.0" stylesheets="@LoginStyle.css" text="Login">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Button>
      <Label fx:id="UsernameLable" alignment="CENTER" contentDisplay="CENTER" layoutX="64.0" layoutY="45.0" prefHeight="47.0" prefWidth="216.0" styleClass="rounded-label" stylesheets="@LoginStyle.css" text="Username">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="PasswordLable" alignment="CENTER" layoutX="64.0" layoutY="111.0" prefHeight="47.0" prefWidth="216.0" styleClass="rounded-label" stylesheets="@LoginStyle.css" text="Password">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="PasswordTextField" layoutX="302.0" layoutY="118.0" prefHeight="34.0" prefWidth="216.0" promptText="ex:JHBUYCns763h2" styleClass="rounded-textfield" stylesheets="@LoginStyle.css" />
      <Button fx:id="RegisterButton" layoutX="215.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="49.0" prefWidth="165.0" text="Register">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Button>
      <Button fx:id="DeleteAccountButton" layoutX="14.0" layoutY="321.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="165.0" stylesheets="@LoginStyle.css" text="Delete account">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Button>
      <Button fx:id="ForgetButton" layoutX="414.0" layoutY="321.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="165.0" stylesheets="@LoginStyle.css" text="Forget password?">
         <font>
            <Font name="Dyuthi Regular" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
